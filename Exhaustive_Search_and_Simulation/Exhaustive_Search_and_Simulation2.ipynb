      "source": [
        "sumStr=input()\n",
        "alpha=\"\"\n",
        "sum=0\n",
        "for i in sumStr:\n",
        "  if i.isalpha():\n",
        "    alpha+=i\n",
        "  else:\n",
        "    sum+=int(i)\n",
        "alpha=sorted(alpha)\n",
        "alpha=\"\".join(alpha)\n",
        "if sum!=0:\n",
        "  result=alpha+str(sum)\n",
        "else:\n",
        "  result=alpha\n",
        "print(result)"
      ]
