        "def rotate_a_matrix_by_90_degree(a):\n",
        "  n=len(a)#행 길이 계산\n",
        "  m=len(a[0])#열 길이 계산\n",
        "  result=[[0]*n for _ in range(m)]\n",
        "  for i in range(n):\n",
        "    for j in range(m):\n",
        "      result[j][n-i-1]=a[i][j]\n",
        "  return result"
      ],
